#include <stdio.h>

int main() {
    int numero, i;

    
    printf("Ingrese un numero entero positivo:\n ");
    scanf("%d", &numero);
    printf("Los factores del numero %d son: ", numero);
    for (i = 1; i <= numero; i++) {
        if (numero % i == 0) {
            printf("%d ", i);
        }
    }

    return 0;
}
